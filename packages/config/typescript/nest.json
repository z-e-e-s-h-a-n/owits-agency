{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "NestJS Config",
  "extends": "./base.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "incremental": false,
    "noEmit": false
  }
}
